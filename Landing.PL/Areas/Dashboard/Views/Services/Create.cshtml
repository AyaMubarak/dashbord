@model ServiceFormVM
@section Header {
    Create Services
}

<div class="col-md-12">
    <div class="card card-primary">
        <div class="card-header">
            <h3 class="card-title">Create Services</h3>
        </div>
        <form asp-action="Create" asp-controller="Services">
            <div class="card-body">
                <div class="form-group">
                    <label asp-for="Name">Enter Service Name</label>
                    <input asp-for="Name" class="form-control" placeholder="Enter Service Name">
                </div>  
                <div class="form-group">
                    <label asp-for="Description">Enter Service Description</label>
                    <input asp-for="Description" class="form-control" placeholder="Enter Service Description">
                </div>
                <div class="form-group">
                    <label asp-for="CreatedAt">Enter Service Created At</label>
                    <input asp-for="CreatedAt" class="form-control" placeholder="Enter Service CreatedAt">
                </div>

            </div>

            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Create</button>
            </div>
        </form>
    </div>
  


</div>

